# Local environment configuration for single-user JAX CEO mode
# This file replaces cloud deployment settings with local-only configuration

# Model API keys - only add the ones you need
# Get your GROQ API Key here - https://console.groq.com/keys
GROQ_API_KEY=YOUR_GROQ_API_KEY_HERE

# Get your Open AI API Key
OPENAI_API_KEY=YOUR_OPENAI_API_KEY_HERE

# Get your Anthropic API Key
ANTHROPIC_API_KEY=YOUR_ANTHROPIC_API_KEY_HERE

# Get your Google Generative AI API Key
GOOGLE_GENERATIVE_AI_API_KEY=YOUR_GOOGLE_AI_API_KEY_HERE

# You only need this environment variable set if you want to use oLLAMA models
# Use http://127.0.0.1:11434 instead of localhost
OLLAMA_API_BASE_URL=http://127.0.0.1:11434

# Include this environment variable for more logging when debugging locally
VITE_LOG_LEVEL=info

# Single user mode flag - disables multi-user features
VITE_SINGLE_USER_MODE=true

# Disable Cloudflare deployment features
VITE_DISABLE_CLOUDFLARE=true

# Local preview as production
VITE_USE_LOCAL_PREVIEW=true
